{
    "name": "Google Bookmarks",
    "version": "0.4",
    "update_url": "http://clients2.google.com/service/update2/crx",
    "description": "Google Bookmarks extension for chrome",
    "default_locale": "en",
    "background_page": "bg.html", 
    "icons": { "48": "img/icon_48.png" },
    "permissions": ["tabs", "https://*.google.com/"],
    "browser_action": {
        "default_title": "Bookmark",
        "default_icon": "img/error.png",
        "popup": "popup.html"
    }
}
